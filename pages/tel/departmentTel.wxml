<!--pages/tel/departmentTel.wxml-->
<!-- 学校各系部电话号码通讯录专页 -->
<view class="page">
  <view class="page__bd">
    <view class="weui-cells weui-cells_after-title">
      <view wx:for="{{telNumber}}" wx:key="telList" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__hd">
          <image src="/images/tel.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
        </view>
        <view class="weui-cell__bd">{{item.name}}</view>
        <view class="weui-cell__ft weui-cell__ft_in-access" id='{{item.tel}}' catchtap='callPhone' catchlongpress="copyIt">{{item.tel}}</view>
      </view>
    </view>
  </view>
  <view class="weui-cells__tips">外地用户记得加拨天津长途区号022</view>
  <view class="weui-btn-area">
    <button plain='true' open-type="share" type="default" over-class="other-button-hover">
      <image class="weui-btn btnIcon" src='/images/share.png'></image>
      <text>分享给朋友</text>
    </button>
  </view>
  <view class="weui-footer">
    <view class="weui-footer__text">Copyright © 2018 Airmole Cheung.</view>
  </view>
</view>